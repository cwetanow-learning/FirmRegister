@model IEnumerable<FirmRegister.Domain.Models.ApplicationUser>

@{
    ViewBag.Title = "Results";
}

@if (Model.Count() == 0)
{
    <h2>Your search returned no results</h2>
}
else
{
    <h2>Results</h2>

    foreach (var item in Model)
    {
        Html.RenderPartial("_ShortenedEmployeeInfo", item);
    }

}
